<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--题库的(滑动式抽屉)-->
    <declare-styleable name="VerticalSlideLayout">
        <attr name="sliding_handle" format="reference"/>
        <!--起始排版权重位置-->
        <attr name="sliding_weight" format="float"/>
        <!--最小可拖动位置-->
        <attr name="slidingStartMinimumHeight" format="dimension"/>
        <!--最大可拖动位置-->
        <attr name="slidingEndMinimumHeight" format="dimension"/>
    </declare-styleable>
    <!--题库的(滑动式抽屉)-->
    <declare-styleable name="ExSlidingLayout">
        <attr name="ex_sliding_handle" format="reference"/>
        <!--起始排版权重位置-->
        <attr name="ex_sliding_weight" format="float"/>
        <!--最小可拖动位置-->
        <attr name="ex_slidingStartMinimumHeight" format="dimension"/>
        <!--最大可拖动位置-->
        <attr name="ex_slidingEndMinimumHeight" format="dimension"/>
    </declare-styleable>

</resources>